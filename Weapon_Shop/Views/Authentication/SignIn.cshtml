@model Weapon_Shop.Feature.Authentication.SignIn.Command

<form asp-action="SignIn" asp-controller="Authentication" asp-anti-forgery="true">
    <div class="form-group">
        <label asp-for="Email">Введите UserName</label><br />
        <input type="text" asp-for="Email" />
    </div>
    <div class="form-group">
        <label asp-for="Password">Введите пароль</label><br />
        <input asp-for="Password" />
        <span asp-validation-for="Password" />
    </div>
    <input class="btn btn-primary" type="submit" value="Войти" />
</form>
